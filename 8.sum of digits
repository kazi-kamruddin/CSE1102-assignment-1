#include <stdio.h>

int main () {

    int x,sum=0;
    printf ("Enter a number (5 digits): ");
    scanf ("%d", &x);

    sum = x%10;
    x = x/10;
    sum += x%10;
    x = x/10;
    sum += x%10;
    x = x/10;
    sum += x%10;
    x = x/10;
    sum += x%10;

    printf ("\nSum of digits: %d\n", sum);
    return 0;
}

